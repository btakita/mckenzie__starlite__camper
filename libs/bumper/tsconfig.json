{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"composite": true,
		"declaration": true,
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"lib": [
			"ESNext",
			"dom"
		],
		"paths": {
			"*": [
				"./node_modules/*"
			]
		},
		"skipLibCheck": true,
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"exclude": []
}
